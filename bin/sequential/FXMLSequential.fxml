<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sequential.SequentialController"> 
   <bottom>
      <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="900.0" spacing="6.0" style="-fx-background-color: #152449;" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="3.0" />
         </padding>
         <children>
            <TextField fx:id="text_field" alignment="CENTER" prefHeight="30.0" prefWidth="200.0" promptText="Input" style="-fx-border-color: #1f91ef; -fx-border-width: 4;" translateX="150.0" />
            <Button mnemonicParsing="false" onAction="#insertOnAction" prefHeight="20.0" prefWidth="70.0" style="-fx-background-color: #152449; -fx-border-width: 3; -fx-border-color: #0435f7;" text="Insert" textFill="WHITE" translateX="180.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#deleteOnAction" prefHeight="20.0" prefWidth="70.0" style="-fx-background-color: #152449; -fx-border-width: 3; -fx-border-color: #0435f7;" text="Delete" textFill="WHITE" translateX="183.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#searchOnAction" prefHeight="20.0" prefWidth="70.0" style="-fx-background-color: #152449; -fx-border-width: 3; -fx-border-color: #0435f7;" text="Demo" textFill="WHITE" translateX="186.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#clearOnAction" prefHeight="20.0" prefWidth="70.0" style="-fx-background-color: #152449; -fx-border-width: 3; -fx-border-color: #0435f7;" text="Clear" textFill="WHITE" translateX="189.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#backOnAction" prefHeight="10.0" prefWidth="60.0" style="-fx-background-color: #152449; -fx-border-width: 3; -fx-border-color: #0435f7;" text="Back" textAlignment="CENTER" textFill="WHITE" translateX="310.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </bottom>
   <center>
      <Pane fx:id="pane" prefHeight="470.0" prefWidth="900.0" style="-fx-background-color: #27297a;" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
